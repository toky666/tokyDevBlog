{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
	  "paths": {
		"@src/*": ["src/*"],
		"@components/*": ["src/components/*"],
		"@pages/*": ["src/pages/*"],
		"@styles/*": ["src/styles/*"],
		"@img/*": ["public/img/*"]
	  },
  }
}